        "\n",
        "await Deno.jupyter.display(\n",
        "  {\n",
        "    \"text/markdown\": \"```ts\\n\\n\" + m + \"\\n```\",\n",
        "  },\n",
        "  { raw: true },\n",
        ");\n",
        "await Deno.jupyter.display(\n",
        "  {\n",
        "    \"text/markdown\": \"```ts\\n\\n\" + tests + \"\\n```\",\n",
        "  },\n",
        "  { raw: true },\n",
        ");"
      ],
